{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "env": {
        "browser": true, 
        "node": true
    },
    "rules": {
        "prettier/prettier": ["error", {
            "singleQuote": true
        }],
        "react/jsx-filename-extension": [0, { "allow": "always" }],
        "react/jsx-one-expression-per-line": [0, { "allow": "none" }]
    }
}